{
  "hosting": {
    "headers": [
      {
       "source": "**/*",
       "headers": [
         {
           "key": "Cross-Origin-Opener-Policy",
           "value": "same-origin"
         },{
           "key": "Access-Control-Allow-Origin",
           "value": "*"
         },{
           "key": "Cross-Origin-Resource-Policy",
           "value": "cross-origin"
         },
         {
           "key": "Cross-Origin-Embedder-Policy",
           "value": "require-corp"
         }
       ]
     },
     {
      "source": "**/*.@(mjs|js)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
     }
    ],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    } ]
  }
}
