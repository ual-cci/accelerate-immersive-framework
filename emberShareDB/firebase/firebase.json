{
  "hosting": {
    "headers": [ {
      "source": "**/*.@(js|mjs)",
      "headers": [ {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      },{
        "key": "Cross-Origin-Opener-Policy",
        "value": "same-origin"
      },{
        "key": "Cross-Origin-Embedder-Policy",
        "value": "require-corp"
      },{
        "key": "Cross-Origin-Resource-Policy",
        "value": "cross-origin"
      }
      ]
    } ],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    } ]
  }
}
